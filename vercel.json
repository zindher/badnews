{
  "name": "badnews-frontend",
  "version": 2,
  "public": false,
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false,
    "autoJobCancelation": true
  },
  "buildCommand": "cd frontend && npm run build",
  "devCommand": "cd frontend && npm run dev",
  "installCommand": "cd frontend && npm ci",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "cleanUrls": true
}
